	file = {Chawla et al_2009_A method for automatic detection and classification of stroke from brain CT.pdf:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/HUI44EIA/Chawla et al_2009_A method for automatic detection and classification of stroke from brain CT.pdf:application/pdf;Chawla et al. - 2009 - A method for automatic detection and classificatio.pdf:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/ZZ6EW9ET/Chawla et al. - 2009 - A method for automatic detection and classificatio.pdf:application/pdf;IEEE Xplore Abstract Record:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/TBS7DBGJ/abs_all.html:text/html;IEEE Xplore Abstract Record:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/VZKCNSHR/login.html:text/html}
