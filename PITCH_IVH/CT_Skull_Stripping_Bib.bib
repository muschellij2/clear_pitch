	keywords = {30-day mortality, hypertension, intracerebral hemorrhage, prognostic indicators, South Asia},
	file = {ScienceDirect Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/MH793GQH/Razzaq and Hussain - 1998 - Determinants of 30-day mortality of spontaneous in.pdf:application/pdf;ScienceDirect Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/NZTEBUAQ/S0090301998000895.html:text/html}
