	langid = {english},
	note = {00006 {PMID}: 23704111},
	keywords = {gradient echo, hematoma volume, Intracerebral hemorrhage, Magnetic Resonance Imaging, quantitative susceptibility mapping},
	file = {Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/NPMHDUQH/Wang et al. - 2013 - Hematoma Volume Measurement in Gradient Echo MRI U.pdf:application/pdf;Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/5TJ4JKM4/2315.html:text/html}
