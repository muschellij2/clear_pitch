	author = {Mendelow, A David and Gregson, Barbara A and Rowan, Elise N and Murray, Gordon D and Gholkar, Anil and Mitchell, Patrick M},
	urldate = {2014-12-09},
	date = {2013-08-09},
	note = {00084},
	file = {ScienceDirect Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/KR9JECAA/Mendelow et al. - 2013 - Early surgery versus initial conservative treatmen.pdf:application/pdf;ScienceDirect Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/4SQU9URH/S0140673613609861.html:text/html}
