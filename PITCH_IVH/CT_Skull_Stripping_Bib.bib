@article{phan_development_2010,
	title = {Development of a new tool to correlate stroke outcome with infarct topography: A proof-of-concept study},
	volume = {49},
	issn = {1053-8119},
	url = {http://www.sciencedirect.com/science/article/pii/S1053811909008568},
	doi = {10.1016/j.neuroimage.2009.07.067},
	shorttitle = {Development of a new tool to correlate stroke outcome with infarct topography},
	abstract = {Improving the ability to assess potential stroke deficit may aid the selection of patients most likely to benefit from acute stroke therapies. Methods based only on ‘at risk’ volumes or initial neurological condition do predict eventual outcome, but not perfectly. Given the close relationship between anatomy and function in the brain, we performed a proof-of-concept study to examine how well stroke outcome correlated with infarct location and extent. A prospective study of 60 patients with ischemic stroke (38 in the training set and 22 in the validation set), using an implementation of partial least squares with penalized logistic regression ({PLS}-{PLR}), was performed. The method yielded a model relating location of infarction (on a voxel-by-voxel basis) and neurological deficits. The area under the receiver operating characteristics curve ({AUC}) method was used to assess the accuracy of the method for predicting outcome. In the validation phase, this model indicated the presence of neglect ({AUC} 0.89), aphasia ({AUC} 0.79), right-arm motor deficit (0.94), and right-leg motor deficit ({AUC} 0.94) but less accurately indicated left-arm motor deficit (0.52) and left-leg motor deficit (0.69). The model indicated no to mild disability (Rankin ≤ 2) versus moderate to severe disability (Rankin \&gt; 2) with {AUC} 0.78. In this proof-of-concept study, we have demonstrated that stroke outcome correlates well with infarct location raising the possibility of accurate prediction of neurological deficit in the individual stroke patient using only information on infarct location and multivariate regression methods.},
	pages = {127--133},
	number = {1},
	journaltitle = {{NeuroImage}},
	shortjournal = {{NeuroImage}},
	author = {Phan, Thanh G. and Chen, Jian and Donnan, Geoffrey and Srikanth, Velandai and Wood, Amanda and Reutens, David C.},
	urldate = {2014-11-12},
	date = {2010-01-01},
	note = {00019},
	keywords = {Atlas, Digital, Prediction, stroke outcome},
	file = {ScienceDirect Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/5GZHEAIG/Phan et al. - 2010 - Development of a new tool to correlate stroke outc.pdf:application/pdf;ScienceDirect Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/B9Z2QZNN/S1053811909008568.html:text/html}
}

@article{phan_hydrocephalus_2000,
	title = {Hydrocephalus Is a Determinant of Early Mortality in Putaminal Hemorrhage},
	volume = {31},
	issn = {0039-2499, 1524-4628},
	url = {http://stroke.ahajournals.org/content/31/9/2157},
	doi = {10.1161/01.STR.31.9.2157},
	abstract = {Background and Purpose—Previous studies have shown that the volume of intracerebral hemorrhage and Glasgow Coma Score ({GCS}) on admission are powerful predictors of 30-day mortality. However, the significance of hydrocephalus associated with deep cerebral hemorrhage has not been studied extensively. The purpose of this study was to determine the prognostic indicators of 30-day mortality in patients with deep cerebral hemorrhage.
Methods—We studied 100 consecutive patients with deep cerebral hemorrhage between 1994 and 1998. Deep cerebral hemorrhage was divided into 2 groups: putaminal hemorrhage (lateral group) and thalamic and caudate hemorrhage (medial group). Univariate and multivariate logistic regression analyses were performed to determine independent prognostic indicators of 30-day mortality.
Results—Hydrocephalus was present in 40 of the 100 patients. The 30-day mortality was 29\%, and hydrocephalus was present in 76\% of those who died. Multivariate analyses showed 2 independent prognostic indicators of 30-day mortality for putaminal hemorrhage: {GCS} ≤8 (P=0.002, odds ratio [{OR}] 37.7, {CI} 3.6 to 396.9) and hydrocephalus (P=0.005, {OR} 27.4, {CI} 2.7 to 282.6). However, only {GCS} ≤8 (P=0.0002, {OR} 16.5, {CI} 3.7 to 73.4) was predictive of 30-day mortality for thalamic and caudate hemorrhage. This model ({GCS} ≤8 and hydrocephalus) has a sensitivity of 57\% and a specificity of 91\% for predicting 30-day mortality for putaminal hemorrhage. When both attributes were present in putaminal hemorrhage ({GCS} ≤8 and hydrocephalus), 1 (11\%) of 9 patients survived, and when both attributes were missing 28 (100\%) of 28 patients survived.
Conclusions—Obstructive hydrocephalus on admission in a comatose patient with a putaminal hemorrhage predicts 30-day mortality.},
	pages = {2157--2162},
	number = {9},
	journaltitle = {Stroke},
	shortjournal = {Stroke},
	author = {Phan, Thanh G. and Koh, Merian and Vierkant, Robert A. and Wijdicks, Eelco F. M.},
	urldate = {2014-04-23},
	date = {2000-09-01},
	langid = {english},
	pmid = {10978045},
	keywords = {Cerebral Hemorrhage, hydrocephalus, mortality, prognosis},
	file = {Full Text PDF:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/NDSG9KKT/Phan et al. - 2000 - Hydrocephalus Is a Determinant of Early Mortality .pdf:application/pdf;Snapshot:/Users/johnmuschelli/Library/Application Support/Zotero/Profiles/9yinbn16.default/zotero/storage/AAXA9QIM/2157.html:text/html}
}

